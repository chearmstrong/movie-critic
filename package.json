{
  "name": "skill-movie-critic",
  "version": "2.2.0",
  "description": "Get movie ratings, synopsis and suggestions from movie critic",
  "scripts": {
    "deploy:dev": "sls deploy --stage dev --aws-profile personal",
    "deploy:prod": "sls deploy --stage prod --aws-profile personal",
    "update-models": "node ./models/update-models",
    "release": "release",
    "test": "jest",
    "debug:recommend:no-genre": "EVENT=recommendNoGenre node ./debug",
    "debug:recommend:genre": "EVENT=recommendGenre node ./debug",
    "debug:tell-me": "EVENT=tellMe node ./debug",
    "lint": "eslint src"
  },
  "dependencies": {
    "ask-sdk": "^2.7.0",
    "aws-sdk": "^2.939.0",
    "dotenv": "^7.0.0",
    "needle": "^2.4.1",
    "ramda": "^0.26.1",
    "safe-access": "^0.1.0",
    "ytdl-core": "^0.29.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-destructuring": "^7.10.4",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "eclint": "^2.8.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jest": "^24.9.0",
    "lint-staged": "^10.4.0",
    "prettier": "^1.19.1",
    "release": "^6.3.0",
    "serverless": "^1.82.0",
    "serverless-webpack": "^5.3.5",
    "virtual-alexa": "^0.7.5",
    "webpack": "^3.10.0"
  },
  "main": "src/index.js",
  "author": "Che Armstorng",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/chearmstrong/movie-critic.git"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
